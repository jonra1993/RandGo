<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="menu2theme" parent="AppTheme"/>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Tema_inicio" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorIniGradientStart</item>
        <item name="colorPrimaryDark">@color/colorIniGradientEnd</item>
        <item name="colorAccent">@color/colorIniGradientEnd</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:actionBarSize">70dp</item>
        <item name="actionBarSize">70dp</item>
        <item name="android:windowTitleSize">70dp</item>

    </style>
</resources>